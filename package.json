    "@mdx-js/mdx": "^3.1.0",
    "eslint": "^9.24.0",
    "gh-pages": "^6.3.0",
    "globals": "^16.0.0",
    "remark-cli": "^12.0.1",
    "remark-lint": "^10.0.1",
    "remark-mdx": "^3.1.0",
    "remark-preset-lint-recommended": "^7.0.1",
    "stylelint": "^16.18.0",
